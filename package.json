{
  "name": "seventv-app",
  "version": "1.6.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "dev:build": "ng build --configuration=dev-sw",
    "watch": "ng build --watch --configuration=dev-sw",
    "dockerize": "ng build --configuration production && docker build . -t registry.digitalocean.com/xiantis/7tv-app",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run seventv-app:serve-ssr",
    "serve:ssr": "node dist/seventv-app/server/main.js",
    "build:ssr": "ng build --configuration=production ---aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --optimization --aot && ng run seventv-app:server:production",
    "build-stage:ssr": "ng build --configuration=stage ---aot --vendor-chunk --common-chunk --delete-output-path --build-optimizer --optimization --aot && ng run seventv-app:server:stage",
    "prerender": "ng run seventv-app:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/localize": "~12.1.1",
    "@angular/material": "^12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/platform-server": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@angular/service-worker": "~12.1.1",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@nguniversal/express-engine": "^12.1.0",
    "bootstrap": "^4.5.0",
    "color": "^3.1.3",
    "compression": "^1.7.4",
    "date-fns": "^2.20.1",
    "express": "^4.15.2",
    "ngx-cookie-service": "^11.0.2",
    "ngx-markdown": "^12.0.1",
    "rxjs": "~6.6.0",
    "ssr-window": "^3.0.0",
    "superagent": "^6.1.0",
    "tslib": "^2.0.0",
    "twitch-player": "^2.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.1.1",
    "@angular/cli": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@nguniversal/builders": "^12.1.0",
    "@types/color": "^3.0.1",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.6.0",
    "@types/mongodb": "^3.6.8",
    "@types/node": "^12.11.1",
    "@types/superagent": "^4.1.12",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
}
